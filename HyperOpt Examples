    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "r3UETfzH9fYc",
        "outputId": "19c5b3a6-65a5-4d6e-a26c-5b178d780f99"
      },
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "100%|| 100/100 [00:01<00:00, 87.56trial/s, best loss: -0.9800000000000001]\n",
            "best: {'n_neighbors': 5}\n"
          ]
        }
      ],
      "source": [
        "def hyperopt_train_test(params):\n",
        "    clf = KNeighborsClassifier(**params)\n",
        "    return cross_val_score(clf, X, y).mean()\n",
        "\n",
        "space4knn = {\n",
        "    'n_neighbors': hp.choice('n_neighbors', range(1,20))}\n",
        "\n",
        "def f(params):\n",
        "    acc = hyperopt_train_test(params)\n",
        "    return {'loss': -acc, 'status': STATUS_OK}\n",
        "\n",
        "trials = Trials()\n",
        "best = fmin(f, space4knn, algo=tpe.suggest, max_evals=100, trials=trials)\n",
        "print('best:', best)"
      ]
    },
    {
